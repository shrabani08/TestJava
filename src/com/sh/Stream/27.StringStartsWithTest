//27. Write a Java program to count the number of strings in a list that start with a specific letter using streams.
import java.util.*;

import java.util.stream.Collectors;
class StringStartsWithTest {
    public static void main(String[] args) {
        System.out.println("Try java 8");
        
        List<String> strList =Arrays.asList("abc","dWERgh","ghi","abQDR","yrey");
        
       strList.stream().filter(a->a.startsWith("ab")).forEach(System.out::println);
    }
}
