//15. join a list of string space seperated with prefix and a suffix
import java.util.*;

import java.util.stream.Collectors;
class StringJoinTest {
    public static void main(String[] args) {
        System.out.println("Try java 8");
        
        List<String> strList =Arrays.asList("abc,def,ghi");
        
       String str= strList.stream().collect(Collectors.joining(",","pre-","-post"));
       System.out.println(str); 
    }
}
