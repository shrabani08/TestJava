//19. print number of string with length more than 3
import java.util.*;

import java.util.stream.Collectors;
class StringCountwithLengthTest {
    public static void main(String[] args) {
        System.out.println("Try java 8");
        
        List<String> strList =Arrays.asList("abc","defgh","ghi","jdsf","yrey");
       Long cnt =strList.stream().filter(a->a.length()>3).count();
       System.out.println(cnt);
    }
}
